<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/delivery_nav_graph"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.koffi.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_mainFragment_to_termFragment"
            app:destination="@id/termFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.koffi.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" /><action android:id="@+id/action_global_mainFragment" app:destination="@id/mainFragment"/>
    <fragment
        android:id="@+id/termFragment"
        android:name="com.example.koffi.TermFragment"
        android:label="TermFragment" />
</navigation>